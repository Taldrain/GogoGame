<?xml version="1.0"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
<mime-type type="application/x-go-sgf">
<comment>Go Game</comment>
<comment xml:lang="de">Go-Partie</comment>
<magic priority="50">
<match type="string" offset="0:256" value="FF[5]"/>
<match type="string" offset="0:256" value="FF[4]"/>
<match type="string" offset="0:256" value="FF[3]"/>
<match type="string" offset="0:256" value="FF[2]"/>
<match type="string" offset="0:256" value="FF[1]"/>
</magic>
<sub-class-of type="text/plain"/>
<glob pattern="*.sgf"/>
</mime-type>
<mime-type type="application/x-go+xml">
<comment>Go Game</comment>
<comment xml:lang="de">Go-Partie</comment>
<magic priority="80">
<match type="string" offset="0:256" value="&lt;!DOCTYPE Go "/>
<match type="string" offset="0:256" value="&lt;Go&gt;"/>
</magic>
<sub-class-of type="application/xml"/>
<glob pattern="*.xml"/>
</mime-type>
</mime-info>
